include ../../../Build/Makefile_Build

all: wlCreateOpacityTable 

wlCreateOpacityTable: \
	$(Weaklib_TableCreator) wlCreateOpacityTable.o
	$(FLINKER) $(FLAGS) -o wlCreateOpacityTable_$(MACHINE) \
	$(Weaklib_TableCreator) wlCreateOpacityTable.o \
	$(LIBRARIES) 

clean:
	rm -f *.o *.mod *.ld

clobber: clean
	rm wlCreateOpacityTable_$(MACHINE)
